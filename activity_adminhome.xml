<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.lawaware.lawawareapp.admihomepage"



    >


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="         "
        tools:textColor="@color/colorAccent"
        android:fontFamily="monospace"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/imageView"

        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="        "
        tools:textColor="@color/colorAccent"
        android:fontFamily="monospace"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/imageView"

        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Court info  Upload"
        tools:textColor="@color/colorAccent"
        android:fontFamily="monospace"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="100sp"
        android:layout_height="100sp"
        app:srcCompat="@drawable/law1"
        android:id="@+id/imageView2"
        android:layout_below="@+id/textView4"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_alignStart="@+id/imageView3"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3" />

    <ImageView

        app:srcCompat="@drawable/law"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:id="@+id/imageView3"
        android:layout_below="@+id/textView"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Law  Details Upload"
        tools:textColor="@color/colorAccent"
        android:fontFamily="monospace"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/imageView3"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView" />

    <ImageView

        app:srcCompat="@drawable/view"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:id="@+id/imageView4"
        android:layout_above="@+id/imageView"
        android:layout_marginBottom="25dp"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:background="@android:color/background_light" />

    <ImageView

        app:srcCompat="@drawable/log"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:id="@+id/imageView"
        android:layout_marginBottom="13dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" View Law  info"
        tools:textColor="@color/colorAccent"
        android:fontFamily="monospace"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
